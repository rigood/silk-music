mixin member(member, index, page, perPage)
    - const rank = (page -1) * perPage + index + 1
    .member
        .rank
            if rank===1
                span="🥇 "
            if rank===2
                span="🥈 "
            if rank===3
                span="🥉 "
            span=rank+"위"
        .cover
            img(src=member.avatarUrl)
        .name=member.name
        .points=member.points+"점"
        .createdAt=member.createdAt.toLocaleDateString("ko-KR")+" 가입"